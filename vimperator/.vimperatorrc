"3.16.0 (created: 2017/02/01 00:55:15)

source! /home/yasser/.vimperatorrc.local

" vim: set ft=vimperator:
set editor=vim
noremap K H
noremap J L
noremap H gT
noremap L gt

" select all text
inoremap <C-a> <C-Home><C-S-End>
" history navigation
noremap <C-k> <C-o>
noremap <C-j> <C-i>

" moving tabs
map < :tabmove!-1<CR>
map > :tabmove!+1<CR>

" tabs only
set gui=none
set gui=tabs
