AddKeysToAgent yes # Enables ssh clients to use SSH agent
ForwardAgent yes   # Local agent follows your SSH connections
ForwardX11 yes

Host cool
    Hostname ec2-18-209-40-108.compute-1.amazonaws.com
    User ec2-user
    IdentityFile ~/.ssh/cooldown.pem

Host personal
    Hostname 13.51.136.173
    User ubuntu
    IdentityFile ~/.ssh/self-host.pem

Host facebook-bot
    Hostname 16.170.127.25
    User ubuntu
    IdentityFile ~/.ssh/self-host.pem
